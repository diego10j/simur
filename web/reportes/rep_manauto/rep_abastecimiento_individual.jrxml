	<queryString>
		<![CDATA[select * from
(select a.numero,a.mve_placa,a.abastecimiento_total,a.abastecimiento_galones,
(case when (b.abastecimiento_kilometraje-a.abastecimiento_kilometraje) > 0 THEN (b.abastecimiento_kilometraje-a.abastecimiento_kilometraje)
when (b.abastecimiento_kilometraje-a.abastecimiento_kilometraje) is null THEN 0 end ) as kilometros from
(select
(SELECT count (*)
FROM mvabactecimiento_combustible a1
INNER JOIN mv_vehiculo v1 ON a1.mve_secuencial = v1.mve_secuencial
where a1.abastecimiento_kilometraje <= a.abastecimiento_kilometraje and v1.mve_placa = 'PME0093'and a.abastecimiento_anio='2015' and a.abastecimiento_periodo='1') as numero,
a.abastecimiento_kilometraje,v.mve_placa,a.abastecimiento_total,a.abastecimiento_galones
FROM mvabactecimiento_combustible a
INNER JOIN mv_vehiculo v ON a.mve_secuencial = v.mve_secuencial
where v.mve_placa = 'PME0093'and a.abastecimiento_anio='2015' and a.abastecimiento_periodo='1') as a
LEFT JOIN
(select
(SELECT count (*)-1
FROM mvabactecimiento_combustible a1
INNER JOIN mv_vehiculo v1 ON a1.mve_secuencial = v1.mve_secuencial
where a1.abastecimiento_kilometraje <= a.abastecimiento_kilometraje and v1.mve_placa = 'PME0093'and a.abastecimiento_anio='2015' and a.abastecimiento_periodo='1') as numero,
a.abastecimiento_kilometraje,v.mve_placa
FROM mvabactecimiento_combustible a
INNER JOIN mv_vehiculo v ON a.mve_secuencial = v.mve_secuencial
where v.mve_placa = 'PME0093'and a.abastecimiento_anio='2015' and a.abastecimiento_periodo='1') as b
on a.mve_placa=b.mve_placa and a.numero =b.numero
) as p]]>
	</queryString>
