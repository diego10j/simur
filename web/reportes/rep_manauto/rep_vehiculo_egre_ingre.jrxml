	<queryString>
		<![CDATA[SELECT a.mav_secuencial,
a.mve_secuencial,
a.mav_departamento,
a.mav_cargoemplea,
a.mav_nomemplea,
a.mav_nombre_cond,
a.mav_cod_conductor,
a.mav_estado_tramite,
a.mav_autoriza,
a.mav_motivo,
a.mav_observacion,
a.mav_estado_registro,
v.mve_placa,
m.mvmarca_descripcion,
o.mvmodelo_descripcion,
v.mve_ano,
v.mve_color,
a.mav_fechaingreso,
d.mdv_detalle,
d.mdv_cantidad,
d.mdv_estado
FROM mvasignar_vehiculo AS a
INNER JOIN mv_vehiculo v ON a.mve_secuencial = v.mve_secuencial
INNER JOIN mvmarca_vehiculo m ON v.mvmarca_id = m.mvmarca_id
INNER JOIN mvmodelo_vehiculo o ON v.mvmodelo_id = o.mvmodelo_id
INNER JOIN mvdetalle_vehiculo d ON d.mve_secuencial = v.mve_secuencial
WHERE a.mav_estado_tramite = 'Cambio' and a.mve_secuencial = $P{placa} and a.mav_secuencial = $P{codigo}]]>
	</queryString>
