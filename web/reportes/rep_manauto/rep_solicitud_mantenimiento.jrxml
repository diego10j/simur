	<parameter name="numero" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT c.mca_fechainman,
c.mca_responsable,
c.mca_proveedor,
c.mca_autorizado,
c.mca_monto,
c.mca_detalle,
c.mca_kmactual_hora,
c.mca_observacion,
c.mca_acotaciones,
(case when c.mca_tipo_mantenimiento = 'INTERNO' then 'Mecánica Municipal' when c.mca_tipo_mantenimiento = 'EXTERNO' then 'Otros Talleres' end ) AS trabajo,
c.mca_anio,
c.mca_periodo,
v.mve_motor,
v.mve_chasis,
(m.mvmarca_descripcion||'  /  '||
o.mvmodelo_descripcion) AS descripcion,
(case when v.mve_placa is NULL then v.mve_codigo when v.mve_placa is not null then v.mve_placa end ) as placa,
v.mve_departamento,
c.mca_secuencial
FROM mvcab_mantenimiento AS c
INNER JOIN mv_vehiculo AS v ON c.mve_secuencial = v.mve_secuencial
INNER JOIN mvmarca_vehiculo AS m ON v.mvmarca_id = m.mvmarca_id
INNER JOIN mvmodelo_vehiculo o ON v.mvmodelo_id = o.mvmodelo_id
WHERE c.mca_codigo = $P{codigo} AND
c.mve_secuencial = $P{placa} AND
c.mca_secuencial = $P{numero}]]>
	</queryString>
