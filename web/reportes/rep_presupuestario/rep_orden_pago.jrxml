	<queryString>
		<![CDATA[SELECT tes_numero_orden,
(case when tes_proveedor is null then tes_empleado when tes_proveedor is not null then tes_proveedor end) as beneficiario
,(case when tes_proveedor is null then 'Empleado' when tes_proveedor is not null then 'Proveedor' end) as tipo
,tes_valor,tes_valor_letras,tes_asunto,tes_acuerdo,tes_nota,
tes_comprobante_egreso,tes_fecha_ingreso,tes_fecha_comprobante,tes_ide_orden_pago,tes_anio,tes_mes,tes_concepto,tes_estado
FROM tes_orden_pago
WHERE  tes_numero_orden = $P{id_orden} and tes_ide_orden_pago = $P{id_documento}]]>
	</queryString>
