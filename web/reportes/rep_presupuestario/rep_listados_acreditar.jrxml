	<queryString>
		<![CDATA[SELECT
d.ide_detalle_listado,
d.ide_listado,
d.item,
d.comprobante,
d.cedula_pass_beneficiario,
d.nombre_beneficiario,
d.valor,
d.numero_cuenta,
d.codigo_banco,
d.ban_nombre,
d.tipo_cuenta,
d.num_documento,
d.fecha_transferencia,
l.ci_paga,
l.responsable_paga,
l.fecha_listado,
l.ci_envia,
l.responsable_envia
FROM
tes_detalle_comprobante_pago_listado AS d
INNER JOIN tes_comprobante_pago_listado l ON d.ide_listado = l.ide_listado
WHERE d.ide_estado_listado = (SELECT ide_estado_listado FROM tes_estado_listado where estado like 'PAGADO')
and d.num_documento like $P{num_tran} and d.fecha_transferencia = cast ($P{fecha_acre} as date)]]>
	</queryString>
