	<queryString>
		<![CDATA[declare @One integer = 1
SELECT
v.MVE_SECUENCIAL,
v.MVE_MARCA+'  '+
v.MVE_MODELO+'  '+
v.MVE_VERSION AS VEHICULO,
v.MVE_PLACA,
c.MSC_CONDUCTOR
FROM MVVEHICULO v
INNER JOIN MVCABSOLICITUD c ON c.MVE_SECUENCIAL = v.MVE_SECUENCIAL
WHERE v.MVE_PLACA =$P{placa} and @One =$P{numero} and c.MSC_SECUENCIAL = $P{id}]]>
