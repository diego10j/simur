	<queryString>
		<![CDATA[select * from
(select s.*,f.kilometros from
(SELECT DISTINCT
PLACA_VEHICULO
FROM MVORDEN_CONSUMO
where PERIODO = $P{periodo} and
ANIO = $P{anio}) AS s
LEFT JOIN
(SELECT PLACA_VEHICULO,
ROUND(AVG (KILOMETRAJE),2) as kilometros
FROM
dbo.MVCALCULO_CONSUMO
GROUP BY PLACA_VEHICULO)as f
on s.PLACA_VEHICULO = f.PLACA_VEHICULO
) as t
order by kilometros desc]]>
	</queryString>
