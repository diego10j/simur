	<queryString>
		<![CDATA[SELECT
cab.IDE_SOLICITUD_PLACA,
cab.FECHA_SOLICITUD,
cab.CEDULA_RUC_EMPRESA,
cab.NOMBRE_EMPRESA,
cab.DESCRIPCION_SOLICITUD,
cab.NUMERO_AUTOMOTORES,
det.CEDULA_RUC_PROPIETARIO,
det.NOMBRE_PROPIETARIO,
det.NUMERO_FACTURA,
req.IDE_DETALLE_REQUISITOS_SOLICITUD,
req.IDE_DETALLE_SOLICITUD,
req.CONFIRMAR_REQUISITO,
r.DECRIPCION_REQUISITO,
dbo.TRANS_TIPO_SERVICIO.DESCRIPCION_SERVICIO,
dbo.trans_tipo_vehiculo.des_tipo_vehiculo

FROM
dbo.TRANS_SOLICITUD_PLACA AS cab ,
dbo.trans_detalle_solicitud_placa AS det ,
dbo.trans_detalle_requisitos_solicitud AS req ,
dbo.TRANS_TIPO_REQUISITO AS r
INNER JOIN dbo.TRANS_TIPO_SERVICIO ON r.IDE_TIPO_SERVICIO = dbo.TRANS_TIPO_SERVICIO.IDE_TIPO_SERVICIO
INNER JOIN dbo.trans_tipo_vehiculo ON dbo.TRANS_TIPO_SERVICIO.IDE_TIPO_VEHICULO = dbo.trans_tipo_vehiculo.ide_tipo_vehiculo
WHERE
cab.IDE_SOLICITUD_PLACA = det.IDE_SOLICITUD_PLACA AND
det.IDE_detalle_SOLICITUD = req.IDE_detalle_SOLICITUD AND
req.IDE_TIPO_REQUISITO = r.IDE_TIPO_REQUISITO
ORDER BY
det.IDE_SOLICITUD_PLACA ASC,
req.IDE_detalle_SOLICITUD ASC]]>
	</queryString>
