	<queryString>
		<![CDATA[SELECT
fecha_anticipo,
ide_periodo_anticipo_inicial,
ide_periodo_anticipo_final,
valor_anticipo,
valor_cuota_mensual,
(case when valor_cuota_adicional is NOT NULL  then valor_cuota_adicional when valor_cuota_adicional is NULL then '0' end ) AS cuota_adicional,
numero_cuotas_anticipo,
ci_solicitante,
ide_empleado_solicitante,
solicitante,
login_ingre_solicitante
FROM
srh_anticipo
where login_ingre_solicitante like $P{persona}
and fecha_anticipo = cast ($P{pide_fechai} as date)
order by solicitante]]>
	</queryString>
