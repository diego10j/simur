	<queryString>
		<![CDATA[SELECT
 s.ide_solicitud_anticipo,
 s.ide_empleado_solicitante,
 s.ci_solicitante,
 s.solicitante,
 c.valor_anticipo,
 c.numero_cuotas_anticipo,
 c.valor_cuota_mensual,
 c.valor_cuota_adicional,
 p.mes AS mes_descuento,
 s.aprobado_solicitante,
 s.id_distributivo,
 c.ide_periodo_anticipo_inicial,
 c.ide_periodo_anticipo_final
 FROM
 srh_solicitud_anticipo AS s
 INNER JOIN srh_calculo_anticipo AS c ON c.ide_solicitud_anticipo = s.ide_solicitud_anticipo
 INNER JOIN srh_periodo_anticipo AS p ON p.ide_periodo_anticipo = c.ide_periodo_anticipo_inicial
 WHERE c.ide_estado_anticipo = (SELECT ide_estado_tipo FROM srh_estado_anticipo where estado like 'AUTORIZADO')
 order by s.ide_solicitud_anticipo]]>
	</queryString>
