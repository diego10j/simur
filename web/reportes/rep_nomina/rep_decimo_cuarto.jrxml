	<queryString>
		<![CDATA[SELECT
r.valor,
e.tipo,
d.cedula,
d.nombres,
d.fecha_ingreso,
d.id_distributivo_roles,
d.cod_tipo,
(case when d.dias is null then '0' ELSE d.dias end ) AS dias_c,
"p".cod_programa,
"p".nom_programa
FROM
srh_roles AS r ,
srh_tipo_empleado AS e ,
srh_decimo_cuarto_historico AS d ,
prec_programas AS p
WHERE
r.ide_empleado = d.ide_empleado AND
r.ide_programa = p.ide_programa AND
d.cod_tipo = e.cod_tipo and
r.ANO=$P{pide_ano} AND
r.IDE_PERIODO=$P{periodo} AND
r.ID_DISTRIBUTIVO_ROLES=$P{distributivo} AND
r.IDE_COLUMNAS=$P{columnas} AND
r.VALOR >0
GROUP BY p.cod_programa
,d.id_distributivo_roles
,d.cedula
,d.nombres
,r.valor
,e.tipo
,d.fecha_ingreso
,d.cod_tipo
,d.dias
,"p".nom_programa
ORDER BY p.cod_programa,d.nombres asc]]>
	</queryString>
