	<parameter name="SUBREPORT_DIR" class="java.lang.String"/>
	<parameter name="identificacion" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
"a".ide_empleado_solicitante,
"a".ci_solicitante,
"a".solicitante,
"a".fecha_anticipo,
"a".valor_anticipo,
(case when a.valor_cuota_adicional is NOT NULL  then a.valor_cuota_adicional when a.valor_cuota_adicional is NULL then '0' end ) AS cuota_adicional,
d.cuota,
(select (e.mes || '/' || e.anio) from srh_periodo_anticipo e where e.ide_periodo_anticipo = d.ide_periodo_descuento) AS perido_descuento,
(select (e.mes || '/' || e.anio) from srh_periodo_anticipo e where e.ide_periodo_anticipo = d.ide_periodo_descontado) AS periodo_descontado,
d.valor,
(case when d.ide_estado_cuota = 't'  then 'COBRADO' when d.ide_estado_cuota is NULL then 'PENDIENTE' end ) AS estado,
"a".numero_cuotas_anticipo,
"a".numero_cuotas_pagadas,
"a".valor_pagado
FROM
srh_anticipo AS a ,
srh_detalle_anticipo AS d
WHERE
d.ide_anticipo = a.ide_anticipo
and a.ci_solicitante like $P{identificacion}
order by a.ci_solicitante, a.fecha_anticipo,cuota]]>
	</queryString>
